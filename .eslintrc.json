{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-console": "warn",
		"no-unused-vars": "warn"
	},
	"ignorePatterns": ["dist/", "node_modules/"]
}
